{
    "name": "code-and-commits/patch-apply",
    "description": "Generate and apply diffs to strings",
    "type": "library",
    "license": "ISC",
    "authors": [
        {
            "name": "Anthony Porthouse",
            "email": "anthony@porthou.se"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "sebastian/diff": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.49",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "PatchApply\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PatchApply\\": "tests/"
        }
    }
}
